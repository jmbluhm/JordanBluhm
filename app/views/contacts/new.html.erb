	<div class="row">
 	   <div class="col-md-4 col-md-offset-4">
   		<div class="well contact-back">
     	   <%= form_for @contact do |f| %>
       	 	<div class="form-group">
        		<%= f.label :name %>
         	   <%= f.text_field :name, class: 'form-control' %>
       	 	</div>
       	 	<div class="form-group">
         	  	 <%= f.label :email %>
         	  	<%= f.email_field :email, class: 'form-control' %>
        	</div>
        	<div class="form-group">
          	  <%= f.label :message %>
          		<%= f.text_area :message, class: 'form-control' %>
        	</div>
			<div class='text-center'>
       	 	<%= f.submit 'Submit', class: 'btn btn-default btn-light' %>
				<%= link_to "Cancel", welcome_homepage_path(),  class: "btn btn-default btn-dark"%>
			</div>
     	<% end %>
    	</div>
  	</div>
	</div>
</div>

